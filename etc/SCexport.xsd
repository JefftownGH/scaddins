<?xml version="1.0" encoding="Windows-1252"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="SCexport">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" name="FilenameScheme">
                    <xs:complexType>
                        <xs:sequence>
                                <xs:element name="Format" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        </xs:sequence>
                        <xs:attribute name="label" type="xs:string" use="required" />
                        <xs:attribute name="hooks" type="xs:string" use="optional" />
                    </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="PostExportHook">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Command" type="xs:string" minOccurs="0" maxOccurs="1" />
                            <xs:element name="Args" type="xs:string" minOccurs="0" maxOccurs="1"/>
                            <xs:element name="SupportedFileExtensions" type="xs:string" minOccurs="0" maxOccurs="1"/>
                        </xs:sequence>
                        <xs:attribute name="name" type="xs:string" use="required" />
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="name" type="xs:string" use="required" />
        </xs:complexType>
    </xs:element>
</xs:schema>